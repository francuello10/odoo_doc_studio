<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Odoo 19 Style Security using res.groups.privilege -->
    <record model="res.groups.privilege" id="res_groups_privilege_doc_studio">
        <field name="name">Doc Studio</field>
        <field name="sequence">100</field>
        <field name="category_id" ref="base.module_category_productivity"/>
    </record>

    <record id="group_doc_studio_user" model="res.groups">
        <field name="name">User</field>
        <field name="privilege_id" ref="res_groups_privilege_doc_studio"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_doc_studio_manager" model="res.groups">
        <field name="name">Administrator</field>
        <field name="privilege_id" ref="res_groups_privilege_doc_studio"/>
        <field name="implied_ids" eval="[(4, ref('group_doc_studio_user'))]"/>
        <field name="user_ids" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>
